<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean name="ad" class="com.harendra.reference.Address">
		<property  name="id" value="107"/>
		<property name="city" value="Sitamrhi"/>
		
   </bean>
   <bean name="st" class="com.harendra.reference.Student">
	   <property name="name" value="Harendra Ranjan"></property>
	   <property name="address" ref="ad">
		  
	   </property>
   </bean>

</beans>
